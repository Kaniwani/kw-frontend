    "redux-actions": "2.6.5",
    "redux-form": "7.4.2",
    "redux-logic": "3.0.2",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0",
    "sanitize.css": "11.0.1",
    "styled-components": "5.2.0",
    "terser-webpack-plugin": "^3.0.2",
    "voca": "1.4.0",
    "wanakana": "4.0.2",
    "wretch": "1.7.4"
  },
  "devDependencies": {
